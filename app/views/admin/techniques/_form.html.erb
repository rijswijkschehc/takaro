<%= simple_form_for([:admin, @technique], data: { turbo: false }) do |f| %>
  <%= f.input :name %>
  <%= f.input :grip, collection: Technique.grips.keys.map { |w| [w.humanize, w] } %>
  <%= f.input :description, as: :rich_text_area %>
  <%= f.input :video %>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
